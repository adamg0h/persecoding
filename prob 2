a = int(input("Input first positive value: "))
b = int(input("Input second postive value: "))

if a >= 0 and a <= 1000 and b >= 0 and b <= 1000:
    if a == b :
        print("Error")
    else:
        if a > b:
            A = a - b
            print(A)
        else:
            B = b - a
            print(B)
